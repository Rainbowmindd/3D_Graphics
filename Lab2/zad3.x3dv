        children [
            Shape {
                appearance Appearance {
                    texture ImageTexture {
                        url "2k_venus_atmosphere.jpg"
                    }
                }
                geometry Sphere { radius 0.7 }
            }
            ]
        }
      },
      DEF OrbitaZiemia Transform {
        children DEF Ziemia Transform{
        translation 7.5 0 0
        children [
            Shape {
                appearance Appearance {
                    texture ImageTexture {
                        url "2k_earth_daymap.jpg"
                    }
                }
                geometry Sphere { radius 0.65 }
            }
        ]
        }
      },
      DEF OrbitaMars Transform {
