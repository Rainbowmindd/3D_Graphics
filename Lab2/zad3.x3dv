            Shape {
                appearance Appearance {
                    texture ImageTexture {
                        url "2k_mars.jpg"
                    }
                }
                geometry Sphere { radius 0.4 }
            }
        ]
        } 
      },
      DEF OrbitaJowisz Transform {
        children DEF Jowisz Transform{
        translation 10.7 0 0
        children [
