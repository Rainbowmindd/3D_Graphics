#X3D V3.3 utf8

DEF Slonce Transform {
    translation 0 0 0
    children [
        Shape {
            appearance Appearance {
                texture ImageTexture {
                    url "sun.jpg"
                }
            }
            geometry Sphere { radius 2 }
        },
        DEF OrbitaMekury Transform{
            children DEF Merkury Transform {
            translation 3.5 0 0
            children [
                Shape {
                appearance Appearance {
                    texture ImageTexture {
                        url "2k_mercury.jpg"
                    }
                }
                geometry Sphere { radius 0.4 }
            }   
            ]
        }
      },
      DEF OrbitaWenus Transform {
        children DEF Wenus Transform{
