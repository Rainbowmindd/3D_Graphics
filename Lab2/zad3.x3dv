                appearance Appearance {
                    texture ImageTexture {
                        url "2k_jupiter.jpg"
                    }
                }
                geometry Sphere { radius 0.9 }
            }
        ]
      }
      },
      DEF OrbitaSaturn Transform {
        children DEF Saturn Transform{
        translation 12.7 0 0
        children [
            Shape {
                appearance Appearance {
                    texture ImageTexture {
                        url "2k_saturn.jpg"
                    }
                }
                geometry Sphere { radius 0.7 }
            }
        ]
      }
    },
    DEF OrbitaUran Transform {
        children DEF Uranus Transform{
        translation 14.5 0 0
        children [
            Shape {
                appearance Appearance {
                    texture ImageTexture {
